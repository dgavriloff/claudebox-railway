{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "params": {
    "ROOT_PASSWORD": {
      "desc": "The password for the root user. (SSH Access)",
      "optional": false
    }
  }
}
